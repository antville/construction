<% #main %>
$(function () {
  var tape = $('<div>').css({
    position: 'fixed',
    top: 0,
    bottom: 0,
    left: 0,
    width: 15,
    height: '100%',
    background: 'url(data:image/png;base64,)'
  });
  var margin = parseInt($('body').css('margin-left'), 10) || 0;
  $('body').append(tape).css('margin-left', margin + 15);
});
